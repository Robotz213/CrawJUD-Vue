{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "components.d.ts", "./src/@types/**/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "types": ["unplugin-icons/types/vue", "./src/@types", "axios"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "sourceMap": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
